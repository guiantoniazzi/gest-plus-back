{
	"version": "0.2.0",
	"configurations": [
		{
			"type": "node",
			"request": "launch",
			"name": "Launch Program",
			"skipFiles": ["<node_internals>/**"],
			"program": "${workspaceFolder}/dist/index.js",
			"preLaunchTask": "tsc: build - gest-plus-back",
			"outFiles": ["${workspaceFolder}/dist/**/*.js"],
			"internalConsoleOptions": "openOnSessionStart",
		}
	]
}
